{
  "name": "auth-model",
  "version": "1.0.0",
  "description": "A small login and signup MERN project",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.6",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node index.js",
    "server": "nodemon index.js",
